<div id="inner-webpage-content-inventory">
    <form>
        <div class="form-group">
            <label for="store-address">Store Address</label>
            <input type="text" class="form-control" id="store-adress" placeholder="Enter Address of your store">
        </div>
        <div class="form-group">
            <label for="city">City</label>
            <input type="text" class="form-control" id="city" placeholder="Enter City of your store">
        </div>
        <div class="form-group">
            <label for="state">State</label>
            <input type="text" class="form-control" id="city" placeholder="Enter abbreviated state" maxlength=2>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th>Store Id</th>
                    <th>Store Address</th>
                    <th>City</th>
                    <th>State</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>1</td>
                    <td>1 Central Avenue</td>
                    <td>New York City</td>
                    <td>NY</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>